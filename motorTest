int leftmotor = 11;
int rightmotor = 12;
void setup() {
  // put your setup code here, to run once:
    pinMode(leftmotor,OUTPUT);
  pinMode(rightmotor,OUTPUT);
   Serial.begin(9600);
}

void loop() {
  // put your main code here, to run repeatedly:
  digitalWrite(rightmotor,HIGH);
  digitalWrite(leftmotor,HIGH);
  delay(1000);
}
